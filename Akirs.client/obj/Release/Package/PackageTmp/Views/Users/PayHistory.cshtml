    </div>

    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">

                <!-- /.panel-heading -->
                <div class="panel-body">
                    <div class="table-responsive">
                        <table id="history" class="table table-striped table-bordered table-hover Usertable DataTable">
                            <thead>
                                <tr>

                                    <th>UserId</th>
                                    <th>AccountName</th>
                                    <th>Amount</th>
                                    <th>TransactionDate</th>
                                    <th>Status</th>                                  
                                </tr>
                            </thead>
                            <tbody id="historyTab">

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="ajax-loader">
        <img src="~/resources/images/ajax-loader.gif" class="img-responsive" />
    </div>
</div>
