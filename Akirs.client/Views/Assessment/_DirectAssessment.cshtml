<div class="col-lg-12">

    <div class="panel panel-default">

        <div class="panel-heading">

            <i class="fa fa-money"></i>Assessment Information

        </div>
        <div class="panel-body">

            <div class="form-horizontal pull-right">

                <div class="form-group">

                    <label>EnrollmentID:</label>
                    <div class="col-sm-10">
                        <span id="EnrollId">' + response.data.enrollmentID + '</span>

                    </div>

                </div>
                <div class="form-group">

                    <label>First Name:</label>
                    <div class="col-sm-10">

                        <span id="empFullName">' + response.data.EmployeeName + '</span>

                    </div>

                </div>

            </div>

        </div>
        <div class="panel panel-default">

            <div class="panel-heading" style="text-align:center !important">

                Assesment Details

            </div>
            <div class="panel-body">

                <div class="col-lg-6">

                    <div role="form">

                        <div class="form-group">

                            <label>Total Income :</label>
                            <label id="txtincome">' + response.data.Income + '</label>

                        </div>
                        <div class="form-group">

                            <label>20% Gross Income: </label>
                            <label id="txtgrossincome">' + response.data.GrossIncome + '</label>

                        </div>

                    </div>

                </div>
                <!-- /.col-lg-6 (nested) -->
                <div class="col-lg-6">

                    <div role="form">

                        <div class="form-group">

                            <label>Total FPA: </label>
                            <label id="txttotalpfa">' + response.data.TotalFPA + '</label>

                        </div>
                        <div class="form-group">

                            <label>Taxable Income: </label>
                            <label>' + response.data.TaxableIncome + '</label>

                        </div>

                    </div>

                </div>
                <div class="col-lg-6">

                    <div class="form-group">

                        <label>Consolidated Allowance: </label>
                        <label>' + response.data.ConsolidateAllowance + '</label>

                    </div>

                </div>

            </div>

        </div>

    </div>
</div>
