++@model IEnumerable<Akirs.client.DL.TaxType>
@{
    ViewBag.Title = "EnrollmentList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section id="inner-headline">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <ul class="breadcrumb">
                    <li><a href="#"><i class="fa fa-home"></i></a><i class="icon-angle-right"></i></li>
                    <li><a href="#">Features</a><i class="icon-angle-right"></i></li>
                    <li class="active">Pricing box</li>
                </ul>
            </div>
        </div>
    </div>
</section>
<section id="content">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h4>Example on <strong>3 columns</strong></h4>
            </div>
            @foreach (var item in Model)
            {
                <div class="col-lg-4">
                    <div class="pricing-box-alt">
                        <div class="pricing-heading">
                            <h3><strong>@item.Taxtypename</strong></h3>
                        </div>

                        <div class="pricing-content">
                            <ul>
                                <li><i class="icon-ok"></i> 100 applications</li>
                                <li><i class="icon-ok"></i> 24x7 support available</li>
                                <li><i class="icon-ok"></i> No hidden fees</li>
                                <li><i class="icon-ok"></i> Free 30-days trial</li>
                                <li><i class="icon-ok"></i> Stop anytime easily</li>
                            </ul>
                        </div>
                        <div class="pricing-action">
                            <a href="@Url.Action("AddParty", "EnrollmentType", new {rt = item.ItbID })" data-val="@item.ItbID" class="btn btn-medium btn-theme"><i class="icon-bolt"></i> Register now</a>
                        </div>
                    </div>
                </div>
            }

        </div>
        <!-- divider -->
        <div class="row">
            <div class="col-lg-12">
                <div class="solidline">
                </div>
            </div>
        </div>
        <!-- end divider -->

    </div>


</section>
